<?xml version="1.0"?>

<component name="org.nuxeo.ecm.conceptshare.webui.resources.contrib">

  <require>org.nuxeo.web.ui.resources</require>

  <extension target="org.nuxeo.ecm.platform.WebResources" point="resources">
    <resource name="nuxeo-conceptshare-bundle.html" type="import" shrinkable="false">
      <uri>/ui/nuxeo-conceptshare-bundle.html</uri>
    </resource>
  </extension>

  <extension target="org.nuxeo.ecm.platform.WebResources" point="bundles">
    <bundle name="web-ui">
      <resources append="true">
        <resource>nuxeo-conceptshare-bundle.html</resource>
      </resources>
    </bundle>
  </extension>

</component>